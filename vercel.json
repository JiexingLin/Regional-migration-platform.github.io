{
  "version": 2,
  "env": {
    "USE_PYTHON_BACKEND": "true"
  },
  "builds": [
    {
      "src": "migration/next.config.js",
      "use": "@vercel/next"
    },
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "/migration/$1"
    }
  ]
} 